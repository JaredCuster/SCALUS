{
  "Protocols": [
    {
      "Protocol": "rdp"
    },
    {
      "Protocol": "ssh"
    },
    {
      "Protocol":  "telnet" 
    }
  ],
  "Applications": [
    {
      "Id": "windows-rdp",
      "Description": "Windows RDP Client",
      "Name": "Windows RDP Client",
      "Platforms": [ "Windows" ],
      "Protocol": "rdp",
      "Parser": {
        "ParserId": "rdp",
        "Options": [ "waitForInputIdle" ],
        "UseDefaultTemplate": true,
        "PostProcessingExec": "C:/strawberry/perl/bin/perl.exe",
        "PostProcessingArgs": [ "%AppData%/examplePlugin.pl" ]

      },
      "Exec": "C:\\windows\\system32\\mstsc.exe",
      "Args": [ "%GeneratedFile%" ]
    },
    {
      "Id": "windows-rdp-withtemplate",
      "Description": "Windows RDP Client",
      "Name": "Windows RDP Client with template",
      "Platforms": [ "Windows" ],
      "Protocol": "rdp",
      "Parser": {
        "ParserId": "rdp",
        "UseTemplateFile": "%AppData%\\exampleRdpTemplate.rdp",
        "Options": [ "waitForInputIdle" ]
      },
      "Exec": "C:\\windows\\system32\\mstsc.exe",
      "Args": [ "%GeneratedFile%" ]
    },

    {
      "Id": "freerdp",
      "Description": "FreeRDP",
      "Name": "FreeRDP",
      "Platforms": [ "Linux" ],
      "Protocol": "rdp",
      "Parser": {
        "ParserId": "rdp",
        "Options": []
      },
      "Exec": "/usr/bin/xfreerdp",
      "Args": [ "/u:%User%", "/v:%host%:%Port%", "/p:Safeguard" ]
    },
    {
      "Id": "windows-openssh",
      "Description": "Windows OpenSSH",
      "Name": "Windows OpenSSH",
      "Platforms": [ "Windows" ],
      "Protocol": "ssh",
      "Parser": {
        "ParserId": "ssh",
        "Options": []
      },
      "Exec": "C:\\windows\\system32\\cmd.exe",
      "Args": [ "/c", "C:\\Windows\\System32\\OpenSSH\\ssh.exe", "%User%@%Host%" ]
    },

    {
      "Id": "gnome-terminal-ssh",
      "Name": "Gnome Terminal SSH",
      "Description": "Open Gnome Terminal on Linux with the SSH connection information",
      "Platforms": [ "Linux" ],
      "Protocol": "ssh",
      "Parser": {
        "ParserId": "ssh",
        "Options": []
      },
      "Exec": "gnome-terminal",
      "Args": [ "-x", "ssh", "-t", "%User%@%Host%" ]
    },

    {
      "Id": "mac-ssh",
      "Name": "MAC TerminalSSH",
      "Description": "Open Terminal on Mac OS with the SSH connection information",
      "Platforms": [ "Mac" ],
      "Protocol": "ssh",
      "Parser": {
        "ParserId": "ssh",
        "Options": [ "waitforexit" ]
      },
      "Exec": "/usr/bin/open",
      "Args": [ "-b", "com.apple.terminal", "%OriginalURL%" ]
    },
    {
      "Id": "mac-rdp",
      "Name": "Mac RDP",
      "Description": "Run MS Windows RDP on MAC",
      "Platforms": [ "Mac" ],
      "Protocol": "rdp",
      "Parser": {
        "ParserId": "rdp",
        "Options": [ "wait:60" ]
      },
      "Exec": "/usr/bin/open",
      "Args": [
        "-nb",
        "com.microsoft.rdc.macos",
        "%GeneratedFile%"
      ]
    },

    {
      "Id": "putty-ssh",
      "Description": "Putty client",
      "Name": "Putty",
      "Platforms": [ "Windows" ],
      "Protocol": "ssh",
      "Parser": {
        "ParserId": "ssh",
        "Options": []
      },
      "Exec": "C:\\windows\\system32\\cmd.exe",
      "Args": [ "/c", "C:\\Program Files\\PuTTY\\putty.exe", "-ssh", "%user%@%host%" ]
    },
    {
      "Id": "putty-telnet",
      "Description": "Putty client",
      "Name": "Putty Telnet",
      "Platforms": [ "Windows" ],
      "Protocol": "telnet",
      "Parser": {
        "ParserId": "telnet",
        "Options": []
      },
      "Exec": "C:\\windows\\system32\\cmd.exe",
      "Args": [ "/c", "C:\\Program Files\\PuTTY\\putty.exe", "-telnet", "%user%@%host%" ]
    },
    {
      "Id": "remmina-rdp",
      "Description": "Use remmina client on linux",
      "Name": "remmina Rdp client",
      "Platforms": [ "Linux" ],
      "Protocol": "rdp",
      "Parser": {
        "ParserId": "rdp",
        "UseTemplateFile": "%AppData%/remmina.rdp",
        "Options": [ "waitforexit" ]
      },
      "Exec": "/usr/bin/remmina",
      "Args": [ "%GeneratedFile%" ]
    },
    {
      "Id": "remmina-ssh",
      "Description": "Use remmina client on ubuntu",
      "Name": "remmina SSH client",
      "Platforms": [ "Linux" ],
      "Protocol": "ssh",
      "Parser": {
        "ParserId": "ssh",
        "UseTemplateFile": "%AppData%/remmina.ssh",
        "Options": [ "waitforexit" ]
      },
      "Exec": "/usr/bin/remmina",
      "Args": [ "%GeneratedFile%" ]
    }
  ]
}
